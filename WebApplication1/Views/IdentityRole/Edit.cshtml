@model System.Threading.Tasks.Task<Microsoft.AspNetCore.Identity.IdentityRole>

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <form method="post" action="/IdentityRole/Update">
        <input type="hidden" name="Id" value="@Model.Result.Id" />
        <div class="form-group">
            <input type="text" name="Name" value="@Model.Result.Name" />
        </div>
        <div class="form-group">
            <input type="submit" value="Save"/>
            <input type="reset" value="Reset"/>
        </div>
    </form>
</div>
</body>
</html>